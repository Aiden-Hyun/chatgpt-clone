{
  "name": "chatgpt-clone",
  "main": "index.ts",
  "version": "1.0.0",
  "scripts": {
    "start": "expo start",
    "reset-project": "node ./scripts/reset-project.js",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "lint": "npx eslint --ext .ts,.tsx --f ./eslintPreciseFormatter.js",
    "lint:boundaries": "npx eslint src --ext .ts,.tsx --max-warnings 0 -f ./eslintPreciseFormatter.js",
    "check:dup-types": "tsx findTypeClones.ts",
    "check:unused-exports": "ts-unused-exports tsconfig.json | grep src",
    "check:all-types": "npm run check:dup-types && npm run check:unused-exports && npm run check:unused-types",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "typecheck": "tsc --noEmit",
    "check:unused-types": "tsx scripts/findUnusedTypes.ts",
    "check:type-aliases": "tsx scripts/findTypeAliases.ts"
  },
  "dependencies": {
    "@expo/vector-icons": "^15.0.2",
    "@react-native-async-storage/async-storage": "2.2.0",
    "@react-native-google-signin/google-signin": "^14.0.0",
